{
    "name": "@dooz/sdk",
    "version": "0.1.0",
    "description": "TypeScript/Bun SDK for integrating with Dooz Core",
    "type": "module",
    "main": "./src/index.ts",
    "module": "./dist/index.js",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "bun": "./src/index.ts",
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "bun build ./src/index.ts --outdir ./dist --target bun",
        "dev": "bun --watch src/index.ts",
        "test": "bun test",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "bun run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DoozHub/dooz-bun-sdk.git"
    },
    "keywords": [
        "dooz",
        "sdk",
        "api",
        "client",
        "bun",
        "typescript",
        "multi-tenant"
    ],
    "author": "Doozie Software",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DoozHub/dooz-bun-sdk/issues"
    },
    "homepage": "https://github.com/DoozHub/dooz-bun-sdk#readme",
    "devDependencies": {
        "@types/bun": "latest",
        "typescript": "^5.0.0"
    },
    "engines": {
        "bun": ">=1.0.0"
    }
}